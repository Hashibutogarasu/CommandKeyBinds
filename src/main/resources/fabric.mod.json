{
  "schemaVersion": 1,
  "id": "commandkeybind",
  "version": "${version}",
  "name": "Command Keybind",
  "description": "If you pressed your setted keybind,you could execute commands from client",
  "authors": [
    "Hashibutogarasu"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/commandkeybinds/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.Hashibutogarasu.commandkeybind.CommandKeyBindModClient"
    ],
    "modmenu": [
      "com.Hashibutogarasu.commandkeybind.ModmenuIntegration"
    ]
  },
  "mixins": [
    "commandkeybind.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
